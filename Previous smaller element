class Solution:
	# @param A : list of integers
	# @return a list of integers
	def prevSmaller(self, A):
        stack = []
        ans = [-1] * len(A)
        for currIdx in range(len(A) - 1,-1,-1):
            while len(stack) > 0 and A[stack[-1]] > A[currIdx]:
                ans[stack[-1]] = A[currIdx]
                stack.pop()
            stack.append(currIdx)
        return ans
