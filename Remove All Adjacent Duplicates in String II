class Solution:
    def removeDuplicates(self, s: str, k: int) -> str:
        st = []
        for ch in s:
            if len(st) > 0 and st[-1][0] == ch:
                st[-1][1] += 1
                if st[-1][1] == k:
                    st.pop()
            else:
                st.append([ch,1])
        ans = []
        for ch,freq in st:
            while freq > 0:
                ans.append(ch)
                freq -= 1
        return "".join(ans)
